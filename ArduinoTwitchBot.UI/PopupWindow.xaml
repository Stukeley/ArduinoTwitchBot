<Window x:Class="ArduinoTwitchBot.UI.PopupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArduinoTwitchBot.UI"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
		mc:Ignorable="d" ResizeMode="CanMinimize" WindowStyle="ThreeDBorderWindow"
		Title="Arduino Twitch Bot" Height="450" Width="550" TextElement.Foreground="{DynamicResource MaterialDesignBody}" Background="{DynamicResource MaterialDesignPaper}" FontFamily="{materialDesign:MaterialDesignFont}">
    <Grid>
		<StackPanel>
			<Frame x:Name="PageFrame" NavigationUIVisibility="Hidden"/>
		</StackPanel>
	</Grid>
</Window>
